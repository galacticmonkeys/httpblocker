{
  "manifest_version": 2,

  "name": "No Pain Web Surfing",
  "description": "Blocks http requests to google.com for faster web surfing in China",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "webRequest",
    "webRequestBlocking",
    "*://*.google.com/",
    "*://*.googleapis.com/",
    "background"
  ]
}